<p id="notice"><%= notice %></p>

<p>
  <strong>Start:</strong>
  <%= @trip.start %>
</p>

<p>
  <strong>End:</strong>
  <%= @trip.end %>
</p>

<% size = :medium %>
<% count = 0 %>
<% @photos.each do |p| %>
  <%= image_tag p.image_url(size) %>
  <% count += 1 %>
  <% if count % 5 == 0 %>
    <br>
  <% end %>
  
<% end %>

<%= link_to 'Edit', edit_trip_path(@trip) %> |
<%= link_to 'Back', trips_path %>
